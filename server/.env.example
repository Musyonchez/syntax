# SyntaxMem Server Environment Variables
# Copy this file to .env and fill in your actual values

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority
DATABASE_NAME=syntaxmem

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-min-32-characters
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=720

# Google OAuth Configuration (for client-side, but stored here for reference)
GOOGLE_CLIENT_ID=your-google-oauth-client-id
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# CORS Configuration
ALLOWED_ORIGINS=https://syntaxmem.dev,http://localhost:3000

# Function Configuration
FUNCTION_REGION=us-central1
FUNCTION_MEMORY=512MB
FUNCTION_TIMEOUT=540

# Development Configuration
NODE_ENV=production
LOG_LEVEL=info

# Optional: Admin Configuration
ADMIN_EMAIL=your-admin-email@example.com